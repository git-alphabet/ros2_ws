<root BTCPP_format="4">
    <BehaviorTree ID="PerceptionAndBlackboard">
        <Sequence>
            <SubAllRobotHP topic_name="robot_hp"
                         robot_hp="{robot_hp}"/>
            <SubRobotStatus topic_name="robot_status"
                            robot_status="{robot_status}"/>
            <SubGameStatus topic_name="/game_status"
                           game_status="{game_status}"/>
            <SubDecisionNum topic_name="decision_num"
                            decision_num="{decision_num}"/>
            <SubRFIDStatus topic_name="/rfid_status"
                           rfid_status="{rfid.status}"/>
            <InitBlackboardConfig
                        heal_wait_ms="{cfg.heal_wait_ms}"
                        heal_min_ratio="{cfg.heal_min_ratio}"
                        search_timeout_ms="{cfg.search_timeout_ms}"
                        recovery_timeout_ms="{cfg.recovery_timeout_ms}"
                        supply_goal_x="{cfg.supply_goal_x}"
                        supply_goal_y="{cfg.supply_goal_y}"
                        arrive_radius="{cfg.arrive_radius}"/>
            <SubRobotPosition topic_name="/robot_position"
                              pose_x="{pose.x}"
                              pose_y="{pose.y}"
                              pose_yaw="{pose.yaw}"/>
        </Sequence>
    </BehaviorTree>
</root>